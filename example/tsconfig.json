{
  "compilerOptions": {
    "target": "es5",
    "module": "NodeNext",
    "jsx": "react",
    "moduleResolution": "nodenext",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "lib": ["es2015", "es2016", "dom"],
    "baseUrl": ".",
    "types": ["node"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}
